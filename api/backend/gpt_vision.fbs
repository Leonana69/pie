// l4m_vision.fbs
namespace Model.Gpt.Vision;

table EmbedImage {
  embedding_ids:[uint];
  image_blob:[ubyte];
}

table BatchEmbedImage {
  items:[EmbedImage];
}

union Command {
  BatchEmbedImage
}

table Request {
  correlation_id:uint;
  command:Command;
}

table Response {
  correlation_id:uint;
}

root_type Request;